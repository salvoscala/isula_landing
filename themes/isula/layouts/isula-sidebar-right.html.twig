<div {{ attributes.setAttribute('id', settings.css_id).addClass('isula-sidebar-right', settings.css_classes ) }}>
  <div class="top">
    {{ content.top }}
  </div>

  <div class="container">
    <div class="col-xs-12 col-md-8 main-left">
      <div class="content">
        {{ content.main_left }}
      </div>
    </div>

    <div class="col-xs-12 col-md-4 sidebar">
      <div class="content">
        {{ content.sidebar }}
      </div>
    </div>
  </div>

  <div class="container bottom">
    <div class="col-xs-12">
      {{ content.bottom }}
    </div>
  </div>
</div>
